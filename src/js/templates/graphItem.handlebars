<div class="property">
    <h5>
        {{ view.title }}
        {{#if view.timePeriods.length }}
            <span> {{view Ember.Select viewName="timePeriodSelect" contentBinding="view.timePeriods"}}</span>
        {{/if}}
        {{#if view.hasGraph }}
            <span title="save this graph as image" class="save-as-png pure-button"><i class="fa fa-download"></i> download graph</span>
        {{/if}}
    </h5>
    <div class="graph-canvas"></div>
</div>